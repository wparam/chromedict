{
    "name": "Chrome Dict",
    "version": "1.0",
    "description": "This ",
    "background": {
        "scripts": ["b.js"],
        "persistent": false
    },
    "browser_action": {
      "default_popup": "demo.html",
      "default_icon": "dict.png"
    },
    "manifest_version": 2,
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    }
}